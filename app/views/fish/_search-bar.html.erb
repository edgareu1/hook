<%= form_tag fish_index_path, remote: true, method: :get do %>
  <div class="d-flex" style="margin-top: 10px">
    <%= text_field_tag :query,
      params[:query],
      class: "form-control",
      id: "search-input",
      placeholder: "Fish name",
      autocomplete: "off"
    %>
  </div>
<% end %>
