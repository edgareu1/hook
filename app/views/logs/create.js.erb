// If there are any errors in the submited form, then add error messages
<% if @log.errors.any? %>
  document.getElementById('ModalNewLog').innerHTML = "<%= j render "logs/form", log: @log %>";

  displayFormErrors('log_new', 'start_time', `<%= @time_errors.first %>`);
  displayFormErrors('log_new', 'end_time', `<%= @time_errors.first %>`);

  displayFormErrors('log', 'location_id', `<%= @log.errors.full_messages_for(:location).first %>`);
<% end %>
